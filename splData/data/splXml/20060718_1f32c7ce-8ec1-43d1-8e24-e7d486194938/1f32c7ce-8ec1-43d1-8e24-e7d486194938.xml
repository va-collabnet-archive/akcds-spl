<code code="42228-7" codeSystem="2.16.840.1.113883.6.1" codeSystemName="LOINC" displayName="PREGNANCY" />
<title ID="INV-2d039ae2-793d-42b3-95da-b53e7073011c" mediaType="text/x-hl7-title+xml">Pregnancy</title>
<effectiveTime value="20060510" />
